{
  "name": "web-readable-stream",
  "description": "Universal WhatWG ReadableStream implementation",
  "version": "0.0.0-dev",
  "type": "module",
  "main": "./dist/src/lib.cjs",
  "module": "./src/lib.js",
  "exports": {
    ".": {
      "import": "./src/lib.js",
      "require": "./dist/src/lib.cjs"
    },
    "./src/readable.js": {
      "import": "./src/readable.js",
      "browser": "./src/readble.browser.js",
      "require": "./src/readable.cjs"
    },
    "./*": {
      "import": "./src/*.js",
      "require": "./dist/src/*.cjs"
    }
  },
  "scripts": {
    "build:cjs": "rollup --config rollup.config.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --no-semi --write",
      "git add"
    ]
  },
  "devDependencies": {
    "git-validate": "2.2.4",
    "husky": "4.3.7",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "rollup": "2.36.1",
    "rollup-plugin-multi-input": "1.2.0",
    "typescript": "4.1.3"
  }
}
